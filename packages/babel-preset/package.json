{
  "name": "@expressive/babel-preset",
  "version": "0.2.15",
  "description": "Web preset for expressive JSX.",
  "repository": "https://github.com/gabeklein/expressive-dsl",
  "author": {
    "name": "Gabe Klein",
    "email": "gabe@expressive.dev",
    "url": "https://github.com/gabeklein"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "babel",
    "expressive",
    "jsx"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "watch": "tsup src/index.ts --watch --sourcemap inline",
    "test": "vitest",
    "dev": "tsx debug/index.ts",
    "prepublish": "npm run build",
    "preversion": "npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@babel/types": "7.22.5",
    "@expressive/babel-plugin-jsx": "workspace:*",
    "chroma-js": "^2.0.3",
    "easing-coordinates": "^2.0.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/babel__traverse": "^7.20.2",
    "@types/node": "^20.3.1",
    "@types/prettier": " ^2.7.3",
    "prettier": "^2.8.8",
    "tsup": "^8.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.5.0"
  }
}

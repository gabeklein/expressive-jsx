{
  "name": "@expressive/babel-plugin-jsx",
  "version": "1.7.1",
  "description": "React output for XJS babel parser plugin.",
  "repository": "https://github.com/gabeklein/expressive-dsl",
  "author": {
    "name": "Gabe Klein",
    "email": "me@gabeklein.dev",
    "url": "https://github.com/gabeklein"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "babel",
    "expressive",
    "jsx"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/polyfill.js"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "watch": "tsup src/index.ts --watch --sourcemap inline",
    "prepublish": "npm run build",
    "test": "jest",
    "prebuild": "rm -rf lib"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@babel/types": "7.22.5"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/babel__traverse": "^7.20.2",
    "prettier": "^2.8.8",
    "tsup": "^8.0.1"
  },
  "tsup": {
    "external": [
      "../polyfill"
    ]
  }
}

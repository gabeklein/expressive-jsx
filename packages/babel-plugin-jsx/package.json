{
  "name": "@expressive/babel-plugin-jsx",
  "version": "0.2.11",
  "description": "Babel parse plugin for Expressive JSX.",
  "repository": "https://github.com/gabeklein/expressive-dsl",
  "author": {
    "name": "Gabe Klein",
    "email": "gabe@expressive.dev",
    "url": "https://github.com/gabeklein"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "babel",
    "expressive",
    "jsx"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "watch": "tsup src/index.ts --watch --sourcemap inline",
    "test": "vitest",
    "prebuild": "rm -rf dist",
    "prepublish": "npm run build",
    "preversion": "npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/traverse": "^7.22.5",
    "@babel/types": "7.22.5"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/babel__traverse": "^7.20.2",
    "tsx": "^4.19.2"
  }
}

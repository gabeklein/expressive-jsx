{
  "name": "@expressive/eslint-plugin-jsx",
  "version": "0.2.12",
  "description": "An ESLint plugin for JSX",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublish": "npm run build",
    "preversion": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/estree": "^1.0.7",
    "eslint": "^9.26.0"
  },
  "devDependencies": {
    "@types/estree-jsx": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "tsup": "^8.0.1",
    "typescript": "^5.8.3"
  }
}

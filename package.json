{
  "name": "expressive-jsx",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "An alternative syntax for JSX constructs using less code",
  "scripts": {
    "test": "npm run test --prefix packages/babel-plugin-jsx",
    "build": "lerna run build --stream",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "keywords": [
    "react",
    "expressive",
    "syntax"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "jest": "^29.7.0",
    "lerna": "^8.0.0",
    "typescript": "^5.2.2"
  }
}
